# List of commands to be run by crux-test.pl
# Format is these fields separated by '='
#    test name             (printed to the output as a label for the command)
#    standard filename     (path to file with the expected output)
#    command               (command with options and args to be run)
#    ignore string         (regex to ignore for diff command, 're 1' 're 2'

# TESTS WITH NO INDEX

# Generate Peptides
generate_peptides_default = standard_generate_peptides_default.out = ../../../bin/crux-generate-peptides --parameter-file smoke_params2 --output-sequence  test.fasta 

generate_peptides_sort_mass_unique = standard_generate_peptides_mass.out = ../../../bin/crux-generate-peptides --parameter-file smoke_params2 --output-sequence --cleavages all --missed-cleavages --sort mass --redundancy unique test.fasta 

generate_peptides_sort_length_unique = standard_generate_peptides_length.out = ../../../bin/crux-generate-peptides --parameter-file smoke_params2 --output-sequence --cleavages tryptic --missed-cleavages --sort length --redundancy unique test.fasta 

generate_peptides_sort_lexical_unique = standard_generate_peptides_lexical.out = ../../../bin/crux-generate-peptides --parameter-file smoke_params2 --output-sequence --cleavages partial --missed-cleavages --sort lexical --redundancy unique test.fasta 

# Get spectrum
get_ms2_spectrum = standard_get_ms2_spectrum.out = rm -f get_ms2_spectrum.out; ../../../bin/crux-get-ms2-spectrum 2 test.ms2 get_ms2_spectrum.out; cat get_ms2_spectrum.out 

# Search for matches and test sqt file 
search_for_matches_default = standard_search_default.target.sqt = rm -rf target.sqt crux_match_target_test* crux_match_decoy*test* test_crux_index/; ../../../bin/crux-search-for-matches --output-mode all test.ms2 test.fasta; cat target.sqt = 'Time'

# Analyze matches
#  Currently, analyze_matches does not work without an index

# TESTS WITH INDEX

# See that crux-generate-peptides works with an existing index
generate_peptides_use_index_uniq = standard_generate_peptides_use_index_uniq.out = ../../../bin/crux-generate-peptides --parameter-file smoke_params2 --output-sequence --use-index T --redundancy unique copy_test.fasta = '# PROTEIN DATABASE'

# See that crux-search-for-matches works with existing index
search_for_matches_default_existing_index = standard_search_default.target.sqt = rm -rf target.sqt crux_match_target_test* crux_match_decoy*test* ; ../../../bin/crux-search-for-matches --output-mode sqt test.ms2 copy_test.fasta; cat target.sqt = 'Time'

# See that match_analysis works with an existing index and existing results
analyze_matches_existing_index = analysis_results.out =  ../../../bin/crux-analyze-matches good_results copy_test.fasta 

# Create index use crux-generate-peptides to test that it worked

create_index_default = empty_file = rm -rf test_crux_index; ../../../bin/crux-create-index --mass-range 500 test.fasta 

# this does not work saying that it is trying to generate uniq peptides, even with the --redundancy redundant option
#generate_peptides_test_index = standard_generate_peptides_use_index.out = ../../../bin/crux-generate-peptides --parameter-file smoke_params2 --output-sequence --use-index T test.fasta

create_index_uniq = empty_file = rm -rf test_crux_index; ../../../bin/crux-create-index --mass-range 500 --redundancy unique test.fasta 

# use crux-generate-peptides to test that it worked
generate_peptides_test_index_uniq = standard_generate_peptides_use_index_uniq.out = ../../../bin/crux-generate-peptides --parameter-file smoke_params2 --output-sequence --use-index T --redundancy unique test.fasta

# Search for matches and test sqt file
search_for_matches_default_index = standard_search_default.target.sqt = rm -rf target.sqt crux_match_target_test* crux_match_decoy*test* ; ../../../bin/crux-search-for-matches --output-mode all test.ms2 test.fasta; cat target.sqt = 'Time'

# Analyze matches using above index
analyze_matches_index = standard_analyze_index.out =  ../../../bin/crux-analyze-matches . test.fasta 













