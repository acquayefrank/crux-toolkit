DIR=/net/noble/vol1/home/bdiament/tide/test2

$DIR/index_no_enzyme --flagfile=$DIR/params
$DIR/index_partial_digest --flagfile=$DIR/params
$DIR/index_full_digest --flagfile=$DIR/params

$DIR/filter_peptides_full_digest --flagfile=$DIR/params
$DIR/filter_peptides_partial_digest --flagfile=$DIR/params

for i in full partial
do
  ~/tide/read_peptides.py --proteins=$DIR/protix --peptides=$DIR/pepix.$i\_digest >$DIR/peptides.$i\_digest.text
  ~/tide/read_peptides.py --proteins=$DIR/protix --peptides=$DIR/pepix.filtered_$i >$DIR/peptides.filtered_$i.text
done
