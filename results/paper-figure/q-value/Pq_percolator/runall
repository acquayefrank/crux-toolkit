#!/bin/bash
#------------
. ../SOURCEME
#------------

#------------
export OUTPUT=${PREFIX}.anal
export COMMAND_FILE=${PREFIX}.cmds
export MASS_RANGE_FILE=`pwd`/mass.ranges

#------------
rm -f crux_match_*
rm -f match_search_*
ln -fs $FASTA; ln -fs $FASTA_INDEX; ln -fs $MS2;


python generate-mass-range.py > $MASS_RANGE_FILE
python create-search-commands.py $MASS_RANGE_FILE $MS2 $FASTA --parameter-file $PARAM_FILE > $COMMAND_FILE
chmod +x $COMMAND_FILE
$COMMAND_FILE
