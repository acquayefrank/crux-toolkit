#!/bin/bash

export PATH=../../../bin/:$PATH

#---
ms2="QTOF.ms2"
rm -rf *.dta
./ms22dta.pl $ms2
python compare_sequest_score_spectrum.py $ms2
mkdir QTOF
mv *eps *png QTOF

